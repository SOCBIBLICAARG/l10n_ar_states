<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_factura_cae" model="ir.ui.view">
            <field name="name">Genera CAE</field>
            <field name="model">invoice.wizard.genera.cae</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Generar CAE">
                    <separator colspan="4" string="Generar CAE" />
                    <button special="cancel" string="Cancelar" icon="gtk-cancel"/>
                    <button name="generar_archivo" string="Genera CAE" type="object" icon="gtk-ok"/>

                    <separator string="Export" colspan="6"/>
 		    <newline/>
		    <field name="archivo_destino" readonly="1"/>
               </form>
            </field>
        </record>


       <act_window name="Generar CAE"
            res_model="invoice.wizard.genera.cae"
            src_model="account.invoice"
            view_mode="form"
            target="new"
            key2="client_action_multi"
            id="action_invoice_genera_cae"/>

    </data>
</openerp>
